*termName:                 xterm-256color
**locale:                   true
*cursorColor:              LightBlue
*pointerShape:             arrow
*pointerColor:             blue
*selectToClipboard:        true
*allowBoldFonts:           false
*faceName:                 Envy Code R:size=10:antialias=true
*metaSendsEscape: true
*allowTitleOps: false
*cutNewline: true
*highlightSelection: true
*fastScroll: true

Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

Emacs.font: Envy Code R-10
Emacs.fontBackend: xft
Emacs.menuBar: off
Emacs.toolBar: -1
Emacs.verticalScrollBars: off


*background: rgb:00/00/00
*foreground: rgb:ff/ff/ff
